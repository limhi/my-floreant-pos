<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.springframework.org/schema/beans 
 http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

 <bean id="httpLoadRequestHandler" class="com.gama.table.dojo.HttpLoadRequestHandler">
        <property name="eventMeterService" ref="eventMeterService">
        </property>
    </bean>

    <bean id="httpUpdateRequestHandler" class="com.gama.table.dojo.HttpUpdateRequestHandler">
        <property name="eventMeterService" ref="eventMeterService">
        </property>
    </bean>
    
      <bean id="httpDeleteRequestHandler" class="com.gama.table.dojo.HttpDeleteRequestHandler">
        <property name="eventMeterService" ref="eventMeterService">
        </property>
    </bean>
    
    
</beans>